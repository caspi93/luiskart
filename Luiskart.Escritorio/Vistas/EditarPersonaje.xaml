<Window x:Class="Luiskart.Escritorio.Vistas.EditarPersonaje"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Luiskart.Escritorio.Vistas"
        mc:Ignorable="d"
        Title="EditarPersonaje" Height="220" Width="300">
    <Grid Margin="0,0,-8,0">
        <Label Content="Nombre" HorizontalAlignment="Center" Margin="10,44,11,0" VerticalAlignment="Top" Width="279" FontWeight="Bold" FontSize="14" HorizontalContentAlignment="Center"/>
        <TextBox x:Name="txtNombre" HorizontalAlignment="Left" Height="23" Margin="59,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>
        <Button x:Name="btnAceptar" Content="Aceptar" HorizontalAlignment="Left" Margin="199,144,0,0" VerticalAlignment="Top" Width="75" Height="29" FontSize="14" FontWeight="Bold" Click="btnAceptar_Click"/>
        <Button Content="Cancelar" HorizontalAlignment="Left" Margin="39,144,0,0" VerticalAlignment="Top" Width="97" Height="29" FontSize="14" FontWeight="Bold"/>

    </Grid>
</Window>
